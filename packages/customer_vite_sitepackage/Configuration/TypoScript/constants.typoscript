##############################
#### FLUID STYLED CONTENT ####
##############################
styles {
    templates {
        layoutRootPath = EXT:customer_vite_sitepackage/Resources/Private/Layouts/ContentElements/
        partialRootPath = EXT:customer_vite_sitepackage/Resources/Private/Partials/ContentElements/
        templateRootPath = EXT:customer_vite_sitepackage/Resources/Private/Templates/ContentElements/
    }
}


############
### PAGE ###
############
page {
    fluidtemplate {
        layoutRootPath = EXT:customer_vite_sitepackage/Resources/Private/Layouts/Page/
        partialRootPath = EXT:customer_vite_sitepackage/Resources/Private/Partials/Page/
        templateRootPath = EXT:customer_vite_sitepackage/Resources/Private/Templates/Page/
    }
    meta {
        description =
        author = Sven Kalbhenn
        keywords =
        viewport = width=device-width, initial-scale=1
        robots = index,follow
        apple-mobile-web-app-capable = no
        compatible = IE=edge
    }
    tracking {
        google {
            trackingID =
            anonymizeIp = 1
        }
    }
}


##############
### CONFIG ###
##############
config {
    no_cache = 1
    removeDefaultJS = 0
    admPanel = 1
    prefixLocalAnchors = all
    headerComment = build by www.skom.de
    sendCacheHeaders = 1
}
